<?xml version="1.0" encoding="UTF-8"?>
<component name="virgo.nano.deployer.component" immediate="true" xmlns:src="http://www.osgi.org/xmlns/src/v1.1.0">
	<implementation class="org.eclipse.virgo.nano.deployer.Activator"/>
	
	<reference name="EventLogger"
		interface="org.eclipse.virgo.medic.eventlog.EventLogger"
		policy="static" 
		cardinality="1..1" 
		bind="bindEventLogger" 
		unbind="unbindEventLogger"/>
		
	<reference name="KernelConfig"
		interface="org.eclipse.virgo.kernel.core.KernelConfig"  
		policy="static" 
		cardinality="1..1" 
		bind="bindKernelConfig" 
		unbind="unbindKernelConfig"/>
		
</component>
